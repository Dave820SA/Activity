@model GrantBusinessLayer.Grant_Category


@{
    ViewBag.Title = "Category Details";
}

<div style="padding-left: 5px">
    <h3>
        Category<small> Details</small></h3>
        <br />
</div>
<form class="form-horizontal">
<div class="Container">
    <div class="row">
        <div class="col-sm-6">
            <fieldset class="scheduler-border">
                @*<h5 class="text-warning">
                    SOP Details</h5>*@
               
                <div class="form-group col-xs-12">
                <div class="display-label">
                        <strong style="font-size: 1.2em">@Html.DisplayNameFor(model => model.Name)</strong>
                    </div>
                    <div class="display-field">
                        <strong style="font-size: 1.5em; color: White">@Html.DisplayFor(model => model.Name)</strong>
                    </div>
                </div>
               
                <div class="form-group col-xs-12">

                </div>
                <div class="form-group col-xs-12">
