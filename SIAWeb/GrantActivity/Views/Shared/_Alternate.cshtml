<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
     <title>@ViewBag.Title</title>
     <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <link href="~/favicon.ico" rel="Shortcut Icon" type="image/x-icon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    @*<meta name="viewport" content="width=device-width" />*@
    @*<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">*@
    @*@Styles.Render("~/font-awesome-4.7.0/css/font-awesome.min.css")*@
    @Styles.Render("~/bootstrap/css/Sandstone.css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")

</head>
<body>
    <div class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">Grant Activity Reporting</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
          <li class="active"><a href="~/Home/Index"><span class="fa fa-home fa-fw"></span>Home
          </a></li>
          <li><a href="~/Category/Index"><i class="fa fa-search"></i> Categories </a></li>
          <li><a href="~/Daily/Index"><i class="fa fa-search"></i> Reporting </a></li>
          <li><a href="~/Home/Contact"><i class="fa fa-phone"></i>Contact </a></li>
          <li><a href="~/Admin/Index"><i class="fa fa-user-circle"></i>Admin </a></li>
       
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</div>
<div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                 @RenderBody()
                <hr />
            </div>
        </div>
    </div>

 <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
     
        @Scripts.Render("~/bootstrap/js/bootstrap.min.js")
        @RenderSection("scripts", required: false)
</body>
</html>
