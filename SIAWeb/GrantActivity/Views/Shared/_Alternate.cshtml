</head>
<body>
    <div class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="~/Home/Index">Grant Admin Reporting</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav navbar-nav">
              <li class="active"><a href="~/Home/Index"><span class="fa fa-home fa-fw"></span> Home
              </a></li>
              <li><a href="~/Home/Contact"><i class="fa fa-phone"></i> Contact </a></li>
              <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button"
                  aria-haspopup="true" aria-expanded="false">Reporting <span class="caret"></span>
              </a>
                  <ul class="dropdown-menu">
                      <li><a href="~/Daily/Index">Enter Admin Item</a></li>
                      <li role="separator" class="divider"></li>
                      <li><a href="~/GrantType/Index">Grant Type</a></li>
                      <li role="separator" class="divider"></li>
                      <li><a href="~/Category/Index">Categories</a></li>
                  </ul>
              </li>
              <li><a href="~/Admin/Index"><i class="fa fa-user-circle"></i> Admin </a></li>
          </ul>
      </div>
      <!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</div>
<div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                 @RenderBody()
                <hr />
            </div>
        </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    @Scripts.Render("~/bootstrap/js/bootstrap.min.js")
    @RenderSection("scripts", required: false)

