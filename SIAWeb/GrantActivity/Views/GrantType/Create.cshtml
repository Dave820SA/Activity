@model GrantBusinessLayer.Grant_GrantType

@{
    ViewBag.Title = "Create";
}

<div style="padding-left: 5px">
    <h4>
        Create <small>New Grant Type</small></h4>
</div>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    
  <form class="form-horizontal">
    <div class="Container">
        <div class="row">
            <div class="col-sm-6">
                <fieldset class="scheduler-border">
                    
                    <div class="form-group col-xs-12">
                        <div class="editor-label">
                            @Html.LabelFor(model => model.GrantType)
                        </div>
                        <div class="editor-label">
                            @Html.TextBoxFor(model => model.GrantType, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.GrantType, "", new { @class = "text-danger" })
                        </div>
                    </div>
                    
                    <div class="form-group col-xs-12">
                        <input type="submit" class="btn btn-primary" value="Create" />
                    </div>
                </fieldset>

                 <div>
                    <strong style="font-size: 1.2em">| @Html.ActionLink("Back to List", "Index")</strong>
                </div>
            </div>
        </div>
    </div>
    </form>
   
}

