<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
     <title>SAPD WEB</title>
     <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <link href="~/favicon.ico" rel="Shortcut Icon" type="image/x-icon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
 
    @Styles.Render("~/Content/font-awesome")
    @Styles.Render("~/Content/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")

</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h3>
                    @Html.ActionLink("SAPD Web", "Index", "Home")</h3>
                <p>
                    <ul class="nav nav-pills">
                        <li><a href="~/Home/Index"><span class="fa fa-home fa-fw"></span> Home </a></li>
                        <li><a href="~/Contact/Index"><i class="fa fa-phone"></i> Contact </a></li>
                        @*<li class="active"><a href="~/Admin/Index"><i class="fa fa-user-circle"></i> Admin </a>*@
                        </li>
                    </ul>
                    <hr />
                </p>
                @RenderBody()
                <hr />
            </div>
        </div>
    </div>
    <div class="container container-fluid">
        <footer>
            <div class="content-wrapper" style="margin-top: 20px">
                <div class="float-left">
                    <p class="text-center">&copy; @DateTime.Now.Year - Activity App - Created by: D.Davidson#1341 & John Riojas</p>
                </div>
            </div>
      </footer>
    </div>
        @Scripts.Render("~/bundles/googlejs")
        @Scripts.Render("~/bundles/bootstrapjs")
        @RenderSection("scripts", required: false)
    
     @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
        @Scripts.Render("~/bootstrap/js/bootstrap.min.js")
        @RenderSection("scripts", required: false)*@
</body>
</html>
