<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
     <title>SIA WEB</title>
    <link href="/favicon.ico" rel="Shortcut Icon" type="image/x-icon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="../../bootstrap/css/cyborg.css" rel="stylesheet" type="text/css" />
    <script src="../../Scripts/modernizr-2.5.3.js" type="text/javascript"></script>
    <link href="../../Content/Site.css" rel="stylesheet" type="text/css" />

   @* @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")*@
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="jumbotron">
                    <h1>
                       @Html.ActionLink("Strategic Intelligence and Analytics", "Index", "Home")</h1>
                    <p>
                        Bootstrap is a free, open-source and is the most popular HTML, CSS, and JavaScript
                        framework developed by twitter for creating responsive web applications.
                    </p>
                    <p>
                        <ul class="nav nav-pills">
                            <li class="active"><a href="../Home/Index"><span class="glyphicon glyphicon-home"></span> 
                                Home </a></li>
                            <li><a href="../Contact/Index"><span class="glyphicon glyphicon-earphone"></span> Contact
                            </a></li>
                            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown"><span
                                class="glyphicon glyphicon-info-sign"></span> About <span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Mission</a></li>
                                    <li><a href="#">Vision</a></li>
                                    <li><a href="#">Products</a></li>
                                </ul>
                            </li>
                        </ul>
                    </p>
                </div>
                @RenderBody()
                <hr />
            </div>
        </div>
    </div>
    <div class="container container-fluid">
        <footer>
            <div class="content-wrapper" style="margin-top: 20px">
                <div class="float-left">
                    <p class="text-center">&copy; @DateTime.Now.Year - SIA Web - Created by: D.Davidson#1341 & John Riojas</p>
                </div>
            </div>
      </footer>
    </div>
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

        @Scripts.Render("~/bootstrap/js/bootstrap.min.js")
        @RenderSection("scripts", required: false)
</body>
</html>
