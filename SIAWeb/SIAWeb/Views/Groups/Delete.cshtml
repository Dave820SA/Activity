@model PersonnelBusinessLayer.GroupTitle

@{
    ViewBag.Title = "Delete";
}
<div style="padding-left: 5px">
    <h3>
        Delete<small> Contact Group</small></h3>
</div>
<hr />
@using (Html.BeginForm()) {

<form class="form-horizontal">
    <div class="Container">
        <div class="row">
            <div class="col-sm-6">
                <fieldset class="scheduler-border">
                    <h5 class="text-danger">
                        Are you sure you want to delete this Group?</h5>
                    <br />
                    <div class="form-group col-xs-12">
                        <div class="display-label">
                            <strong style="font-size: 1.2em">@Html.DisplayNameFor(model => model.Name)</strong>
                        </div>
                        <div class="display-field">
                            <strong style="font-size: 1.5em; color: White">@Html.DisplayFor(model => model.Name)</strong>
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <div class="display-label">
                            <strong style="font-size: 1.2em">@Html.DisplayNameFor(model => model.GroupInfo)</strong>
                        </div>
                        <div class="display-field">
                            <strong style="font-size: 1.5em; color: White">@Html.DisplayFor(model => model.GroupInfo)</strong>
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <div class="display-label">
                            <strong style="font-size: 1.2em">@Html.DisplayNameFor(model => model.VisibleFlag)</strong>
                        </div>
                        <div class="display-field">
                            <strong style="font-size: 1.5em; color: White">@Html.DisplayFor(model => model.VisibleFlag)</strong>
                        </div>
                    </div>
                    <div class="form-group col-xs-12">

                    </div>
                </fieldset>
                <div>
                    <p>
                        <input type="submit" class="btn btn-danger" value="Delete" />
                        <strong style="font-size: 1.2em">| @Html.ActionLink("Back to List", "Index")</strong>
                    </p>
                </div>
            </div>
        </div>
    </div>
</form>
}








