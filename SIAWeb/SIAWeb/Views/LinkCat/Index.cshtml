@model IEnumerable<SIAWebLinksBusinessLayer.WebCategories>

@{
    ViewBag.Title = "Index";
}


<div style="padding-left:60px">
    <h4>Web<small> Categories</small> </h4>
</div>


<div class="container">
    <div class="row">
        <div class="col-lg-6 col-lg-offset-1 col-md-6 col-sm-6 col-xs-12">
        @*<div class="col-xs-12 col-md-9 ">*@

            <span class="h6">@Html.ActionLink("Create New", "Create")&nbsp;&nbsp;|&nbsp;&nbsp;</span><span
                class="h6">@Html.ActionLink("Admin", "Index", "Admin")</span>
            <table class="table table-condensed table-hover">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Name)
                    </th>
                    <th>
                    </th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.WebCategoriesID }) | @*@Html.ActionLink("Details", "Details", new { id=item.WebCategoriesID }) |*@
                            @Html.ActionLink("Delete", "Delete", new { id = item.WebCategoriesID })
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>


