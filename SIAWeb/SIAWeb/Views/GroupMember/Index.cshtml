@model IEnumerable<PersonnelBusinessLayer.GroupMember>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User_User.PIN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_GroupTitle.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberInfo)
        </th>
