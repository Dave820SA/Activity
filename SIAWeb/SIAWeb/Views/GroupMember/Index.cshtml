        <td>
            @Html.DisplayFor(modelItem => item.User_User.PIN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_GroupTitle.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberInfo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisibleFlag)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.GroupMemberID }) |
