@model IECAWeb.Models.IECA_AuditOffice

@{
    ViewBag.Title = "Edit";
}

<div style="padding-left: 5px">
    <h3>
        Office<small> Preforms Case Audits</small></h3>
</div>
<hr />
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

        <form class="form-horizontal">
    <div class="Container">
        <div class="row">
            <div class="col-sm-6">
                <fieldset class="scheduler-border">
                    @Html.HiddenFor(model => model.AuditOfficeID)
                    <div class="form-group col-xs-12">
                        <strong style="font-size: 1.2em">@(String.Format("{0}", Model.Office_Office.Name))</strong>
                    </div>
                    <div class="form-group col-xs-12">
                        <div class="checkbox">
                            <label class="btn btn-primary">
                                @Html.EditorFor(model => model.VisibleFlag, new { htmlAttributes = new { @class = "myEditorFor" } })
                                <strong style="font-size: 1.2em">@Html.LabelFor(model => model.VisibleFlag, new { @class = "control-label" })</strong>
                            </label>
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <input type="submit" class="btn btn-success" value="Submit" />
                        <br />
                        <br />
                        <hr />
                        <a href="@Url.Action("Index", "Office")" class="btn btn-warning btn-sm">Back To Office
                            List <i class="fa fa-share" aria-hidden="true"></i></a>
                    </div>
                </fieldset>
            </div>
        </div>
    </div>
    </form> 
}     

@section Scripts {

   <script type="text/javascript">
       $(document).ready(function () {

       });
    </script>
	}
