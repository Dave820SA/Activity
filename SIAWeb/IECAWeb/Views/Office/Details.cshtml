@model IECAWeb.Models.IECA_AuditOffice

@{
    ViewBag.Title = "Office Details";
}

<div style="padding-left: 5px">
    <h3>
        Office<small> Details</small></h3>
        <br />
</div>
<form class="form-horizontal">
<div class="Container">
    <div class="row">
        <div class="col-sm-6">
            <fieldset class="scheduler-border">
                <div class="form-group col-xs-12">
                    <div class="display-label">
                        <strong style="font-size: 1.2em">@Html.DisplayFor(model => model.Office_Office.Name)</strong>
                    </div>
                </div>
              
                <div class="form-group col-xs-12">
                    <div class="display-label">
                        <strong style="font-size: 1.2em">@Html.DisplayNameFor(model => model.VisibleFlag)</strong>
                    </div>
                    <div class="display-field">
                        <strong style="font-size: 1.5em; color: White">@Html.DisplayFor(model => model.VisibleFlag)</strong>
                    </div>
                </div>
               
                <div class="form-group col-xs-12">

                </div>
                <div class="form-group col-xs-12">
                <a href="@Url.Action("Edit", "Office", new { id = Model.AuditOfficeID })" class="btn btn-info btn-xs">
                                Edit <i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>&nbsp;&nbsp;
                    @*<a href="@Url.Action("Edit", "Office", new { id = Model.AuditOfficeID })" class="btn btn-info btn-xs"></a>*@
                    <a href="@Url.Action("Index", "Office")" class="btn btn-warning btn-xs">
                            Back To List <i class="fa fa-share" aria-hidden="true"></i></a>
                </div>
            </fieldset>
        </div>
    </div>
</div>
</form>



