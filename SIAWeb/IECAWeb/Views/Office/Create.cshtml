@model IECAWeb.Models.IECA_AuditOffice


<div style="padding-left: 5px">
    <h4>
        Add <small>New Office</small></h4>
</div>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    
      <form class="form-horizontal">
    <div class="Container">
        <div class="row">
            <div class="col-sm-6">
                <fieldset class="scheduler-border">
                    <div class="form-group col-xs-12">
                        <div class="editor-label">
                            @Html.LabelFor(model => model.Office_Office.Name)
                        </div>
                        <div class="editor-label">
                            @Html.DropDownList("AuditOfficeID", null, "Select Office", new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.Office_Office.Name, "", new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <div class="editor-label">
                            @Html.LabelFor(model => model.VisibleFlag)
                        </div>
                        <div class="editor-label">
                            @Html.CheckBoxFor(model => model.VisibleFlag)
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <input type="submit" class="btn btn-primary" value="Add Office" />
                    </div>
                </fieldset>
            </div>
        </div>
        <hr />
        <div>
            <a href="@Url.Action("Index", "Office")" class="btn btn-warning btn-sm">Back To List
                <i class="fa fa-share" aria-hidden="true"></i></a>
        </div>
    </div>
    </form>
   
}

@section Scripts {

   <script type="text/javascript">
       $(document).ready(function () {

       });
    </script>

}

